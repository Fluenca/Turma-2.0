<div>
  <div class="calender hidden" data-calendar-target="calendarr">
    <% if current_user.role == "Artist" %>
      <%= render 'shared/calender-artist'%>
    <% else %>
      <%= render 'shared/calender-venue'%>
    <% end %>
  </div>
  <div class="list" data-calendar-target="list">
    <% if current_user.role == "Artist" %>
      <%= render 'shared/list-artist'%>
    <% else %>
      <%= render 'shared/list-venue'%>
    <% end %>
  </div>
</div>
